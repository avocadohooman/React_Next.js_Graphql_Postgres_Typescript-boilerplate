{
  "name": "react_graphql_postgres_typescript-boilerplate",
  "version": "1.0.0",
  "description": "A full-stack project that serves a boilerplate for react, graphql, postgres and typescript.",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "start:ts-node": "ts-node src/index.ts",
    "dev:ts-node": "nodemon --exec ts-node src/index.ts",
    "create:migration": "mikro-orm migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avocadohooman/react_graphql_postgres_typescript-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avocadohooman/react_graphql_postgres_typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/avocadohooman/react_graphql_postgres_typescript-boilerplate#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.5",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.28.2",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "pg": "^8.7.1",
    "type-graphql": "^1.1.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
